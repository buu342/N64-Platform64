#include "osconfig.h"

beginseg
	name    "code"
	flags   BOOT OBJECT
	entry   boot
    address SEGMENTSTART_CODE
    maxsize SEGMENTSIZE_CODE
	stack   STACKSTART_BOOT + STACKSIZE_BOOT
	include "build/codesegment.o"
	include "$(ROOT)/usr/lib/PR/rspboot.o"
	include "$(ROOT)/usr/lib/PR/gspF3DEX2.fifo.o"
endseg

beginwave
	name    "platform64"
	include "code"
endwave